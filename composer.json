{
  "name": "yunhanphp/lumen-require",
  "description": "Lumen 生产环境集成",
  "keywords": [
    "laravel",
    "lumen"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "raygu0101",
      "email": "raygu0101@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/redis": "^5.6",
    "predis/predis": "^1.1",
    "spatie/laravel-permission": "^2.12"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "laravel/lumen-framework": "^5.6",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "Yunhan\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Yunhan\\Tests\\": "tests"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
